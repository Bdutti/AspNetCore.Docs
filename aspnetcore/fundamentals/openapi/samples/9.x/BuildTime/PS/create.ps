# Creates a Web API project, adds ApiDescription.Server
# and displays first few line
$env:ProgramName = "MyOpenApiTest"
$env:Project = "$($env:ProgramName).csproj"
echo $env:ProgramName
dotnet new webapi -n $env:ProgramName
cd $env:ProgramName
dotnet add package Microsoft.Extensions.ApiDescription.Server --version 9.0.0-*
dotnet build
Get-Content obj/$env:ProgramName.json | Select-Object -First 9